<div
  style="z-index: -1;"
  id="carouselExampleSlidesOnly"
  class="carousel slide"
  data-bs-ride="carousel"
>

  <div class="carousel-inner">
    <% for (let index = 1; index <= 8; index++) { %>
      <div class="carousel-item <%= index === 1 ? 'active' : '' %>">
        <img
          src="/images/crops/crop<%= index %>.jpg"
          class="d-block w-100"
          alt="crop-images"
					height="625rem"
					width="100%"
        />
      </div>
    <% } %>
  </div>
</div>
